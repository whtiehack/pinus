{
    "name": "pinus-logger",
    "version": "1.3.14",
    "private": false,
    "main": "./dist/index",
    "types": "./lib/index",
    "files": [
        "dist",
        "lib",
        "index.ts"
    ],
    "scripts": {
        "build": "tsc",
        "start": "yarn run build && node dist/index",
        "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
        "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
        "test": "yarn run build",
        "cov": "nyc mocha",
        "ci": "yarn run test",
        "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-logger lib/"
    },
    "engines": {
        "node": ">=8.5"
    },
    "dependencies": {
        "@types/node": "8.10.59",
        "log4js": "^6.0.0"
    },
    "devDependencies": {
        "mocha": "7.2.0",
        "should": "13.2.3",
        "tslint": "6.1.1",
        "typescript": "3.8.3"
    }
}
